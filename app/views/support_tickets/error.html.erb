<ul>
  <% if !@ticket.nil? %>
    <% @ticket.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  <% elsif @brush.nil? %>
    <li>Invalid brush SKU: <%= params[:brush_sku].inspect %></li>
  <% else %>
    <li>No support ticket found! This is a bug for Ada to fix, please let us know.</li>
  <% end %>
</ul>

<a class="btn btn-primary" href="javascript:void(0)" onclick="window.history.back();" role="button">Back to feedback form</a>
